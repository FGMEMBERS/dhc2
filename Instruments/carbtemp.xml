<?xml version="1.0"?>

<PropertyList>

<path>carbtemp-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>Carbtemp.001</object-name>
        <object-name>CARBneedle</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>CARBneedle</object-name>
        <property>/environment/temperature-degc</property>
        <interpolation>
            <entry><ind> -30.0</ind><dep>0</dep></entry>
            <entry><ind> 0</ind><dep>110</dep></entry>
            <entry><ind>40</ind><dep>260</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>