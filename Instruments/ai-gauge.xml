<?xml version="1.0"?>

<PropertyList>

<path>ai-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>AIface</object-name>
        <object-name>AIring</object-name>
        <object-name>AIneedle</object-name>
        <object-name>AIgauge.001</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>AIneedle</object-name>
        <property>/instrumentation/attitude-indicator/indicated-pitch-deg</property>
        <factor>-0.001</factor>
        <min-m>-0.01575</min-m>
        <max-m>0.01575</max-m>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AIneedle</object-name>
        <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
        <factor>-1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>texrotate</type>
        <object-name>AIface</object-name>
        <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>-1.0</z>
        </axis>
        <center>
            <x>0.25</x>
            <y>0.5</y>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AIring</object-name>
        <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
        <factor>-1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>