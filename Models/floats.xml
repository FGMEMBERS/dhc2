<?xml version="1.0"?>

<PropertyList>

<path>floats.ac</path>

    <animation>
        <type>material</type>
        <object-name>Floats</object-name>
        <object-name>LHrudder</object-name>
        <object-name>RHrudder</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture</texture-prop>
        <texture>paint1.png</texture>
    </animation>

    <animation>
        <name>LeftFrontGear</name>
        <object-name>Lstrut.001</object-name>
        <object-name>Ldamper</object-name>
        <object-name>LFwheel</object-name>
    </animation>

