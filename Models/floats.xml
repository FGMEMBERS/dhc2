            <property>controls/gear/gear-down</property>
        </condition>
        <object-name>Ldamper</object-name>
        <object-name>LFwheel</object-name>
        <property>gear/gear[4]/compression-m</property>
        <factor>400</factor>
        <center>
            <x-m>-3.592</x-m>
            <y-m>1.434</y-m>
            <z-m>-2.209</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

