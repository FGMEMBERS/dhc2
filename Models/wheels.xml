<?xml version="1.0"?>

<PropertyList>

<path>wheels.ac</path>

    <animation>
        <type>material</type>
        <object-name>StrutBase</object-name>
        <object-name>Rstrut</object-name>
        <object-name>Lstrut</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture</texture-prop>
        <texture>paint1.png</texture>
    </animation>

<!--    Gear Animation    -->

    <animation>
        <type>rotate</type>
        <object-name>Lwheel</object-name>
        <object-name>Lstrut</object-name>
        <property>gear/gear[0]/compression-m</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>14.0</dep></entry>
        </interpolation>
        <center>
            <x-m>-0.834</x-m>
            <y-m>-0.650</y-m>
            <z-m>-0.820</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rwheel</object-name>
        <object-name>Rstrut</object-name>
        <property>gear/gear[1]/compression-m</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>14.0</dep></entry>
        </interpolation>
        <center>
            <x-m>-0.834</x-m>
            <y-m>0.650</y-m>
            <z-m>-0.820</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>Rearwheel.strut</object-name>
        <object-name>Rearwheel</object-name>
        <property>gear/gear[2]/compression-m</property>
        <factor>1.0</factor>
        <axis>
            <x>0.217</x>
            <y>0</y>
            <z>0.976</z>
        </axis>
    </animation>

</PropertyList>
