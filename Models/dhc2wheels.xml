<animation>
  <type>noshadow</type>
  <object-name>navlightsoff</object-name>
  <object-name>navlightson</object-name>
  <object-name>LndLight</object-name>
  <object-name>Fastprop</object-name>
  <object-name>Slowprop</object-name>
  </animation>

 <animation>
	<type>shader</type>
    <shader>chrome</shader>
	<texture>Aircraft/dhc2/Models/chrome.rgb</texture>
	<object-name>Throttle.001</object-name>
	<object-name>PropPitch.001</object-name>
	<object-name>Mixture.001</object-name>	
</animation>

<animation>
  <type>select</type>
  <object-name>navlightson</object-name>
  <condition>
    <greater-than>
    <property>/systems/electrical/outputs/nav-lights</property>
    <value>0</value>
  </greater-than>
  </condition>
</animation>

 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
  <object-name>Glass</object-name>
  <object-name>RHFdoor.glass</object-name>
  <object-name>LHFdoor.glass</object-name>
  <object-name>RRglass</object-name>
  <object-name>LRglass</object-name>
  <condition>
    <greater-than>
    <property>/sim/current-view/view-number</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Cargo</object-name>
  <condition>
   <greater-than>
    <property>/yasim/cargo</property>
    <value>0.000</value>
   </greater-than>
  </condition>
 </animation>

  <animation>
  <type>select</type>
  <object-name>Fastprop</object-name>
  <condition>
  <and>
   <greater-than>
    <property>/engines/engine[0]/rpm</property>
    <value>1500</value>
   </greater-than>
   <greater-than>
    <property>/sim/current-view/view-number</property>
    <value>0</value>
   </greater-than>
   </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Slowprop</object-name>
  <condition>
   <less-than>
    <property>/engines/engine/rpm</property>
    <value>1501</value>
   </less-than>
   <greater-than>
    <property>/engines/engine/rpm</property>
    <value>500</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Prop</object-name>
  <condition>
   <less-than>
    <property>/engines/engine/rpm</property>
    <value>501</value>
   </less-than>
  </condition>
 </animation>
 
 <animation>
  <type>spin</type>
  <object-name>Prop</object-name>
