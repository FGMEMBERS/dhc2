    <animation>
        <type>material</type>
        <object-name>redlight</object-name>
        <emission>
            <red>1.0</red>
            <green>0.1</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/nav-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>greenlight</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/nav-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>beacon</object-name>
        <emission>
            <red>1.0</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/controls/lighting/beacon-state/state</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>noshadow</type>
        <object-name>LndLight</object-name>
        <object-name>Fastprop</object-name>
        <object-name>Slowprop</object-name>
        <object-name>SideLogo</object-name>
    </animation>

    <animation>
        <type>shader</type>
        <shader>chrome</shader>
        <texture>Aircraft/dhc2/Models/chrome.rgb</texture>
        <object-name>Spinner</object-name>
        <object-name>Throttle.001</object-name>
        <object-name>PropPitch.001</object-name>
        <object-name>Mixture.001</object-name>
        <object-name>Ant</object-name>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Glass</object-name>
        <object-name>RHFdoor.glass</object-name>
        <object-name>LHFdoor.glass</object-name>
        <condition>
            <greater-than>
                <property>sim/current-view/view-number</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Fastprop</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/rpm</property>
                    <value>1500</value>
                </greater-than>
                <greater-than>
                    <property>sim/current-view/view-number</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Slowprop</object-name>
        <condition>
            <less-than>
                <property>engines/engine/rpm</property>
                <value>1501</value>
            </less-than>
            <greater-than>
                <property>engines/engine/rpm</property>
                <value>500</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Prop</object-name>
        <condition>
            <less-than>
                <property>engines/engine/rpm</property>
                <value>501</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>Prop</object-name>
        <property>engines/engine/rpm</property>
        <center>
            <x-m>2.623</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.083</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>Slowprop</object-name>
        <property>engines/engine/rpm</property>
        <factor>0.5</factor>
        <center>
            <x-m>2.623</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.083</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

