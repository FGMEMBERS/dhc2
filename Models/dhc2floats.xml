  <!--    Retractable Gear    -->

  <animation>
    <type>rotate</type>
    <object-name>Ldamper</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <factor>91.5</factor>
    <center>
      <x-m>-3.599</x-m>
      <y-m>1.434</y-m>
      <z-m>-2.189</z-m>
    </center>
