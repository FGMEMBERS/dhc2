<animation>
  <type>select</type>
  <object-name>navlightson</object-name>
  <condition>
    <greater-than>
    <property>/systems/electrical/outputs/nav-lights</property>
    <value>0</value>
  </greater-than>
  </condition>
</animation>

 <animation>
  <type>select</type>
  <object-name>cabin</object-name>
  <condition>
    <equals>
    <property>/sim/current-view/view-number</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Cargo</object-name>
  <condition>
   <greater-than>
    <property>/yasim/cargo</property>
    <value>0.000</value>
   </greater-than>
  </condition>
 </animation>

  <animation>
  <type>select</type>
  <object-name>Fastprop</object-name>
  <condition>
   <greater-than>
    <property>/engines/engine[0]/rpm</property>
    <value>9999</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Slowprop</object-name>
