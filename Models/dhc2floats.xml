<?xml version="1.0"?>

<PropertyList>

<path>dhc2f.ac</path>

    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/dhc2/Models/Liveries", 10);
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>

    <model>
        <name>Cabin</name>
        <path>Aircraft/dhc2/Models/cabinf.xml</path>
    </model>

    <model>
        <condition>
            <property>/sim/rendering/rembrandt/enabled</property>
        </condition>
        <name>light</name>
        <path>Aircraft/dhc2/Models/light.xml</path>
        <offsets>
            <x-m>-0.758</x-m>
            <y-m>-3.688</y-m>
            <z-m>1.092</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>Aircraft/dhc2/Models/Effects/exhaust.xml</path>
        <name>backfire</name>
        <offsets>
            <x-m>-1.917</x-m>
            <y-m>0.489</y-m>
            <z-m>-0.788</z-m>
        </offsets>
    </model>

    <animation>
        <type>material</type>
        <object-name>Fuselage</object-name>
        <object-name>Rudder</object-name>
        <object-name>Elevator</object-name>
        <object-name>RHaileron</object-name>
        <object-name>LHaileron</object-name>
        <object-name>RHflap</object-name>
        <object-name>LHflap</object-name>
        <object-name>LHfront.door</object-name>
        <object-name>RHfront.door</object-name>
        <object-name>RHrear.door</object-name>
        <object-name>LHrear.door</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture</texture-prop>
        <texture>default.png</texture>
    </animation>

    <animation>
        <condition>
                <property>systems/electrical/outputs/nav-lights</property>
        </condition>
        <type>select</type>
        <object-name>redlight</object-name>
        <object-name>greenlight</object-name>
    </animation>

    <animation>
        <condition>
                <property>systems/electrical/outputs/beacon</property>
        </condition>
        <type>select</type>
        <object-name>beacon</object-name>
    </animation>


   <effect>
        <inherits-from>Aircraft/dhc2/Models/Effects/skin-reflect</inherits-from>
        <object-name>Fuselage</object-name>
        <object-name>Rudder</object-name>
        <object-name>Elevator</object-name>
        <object-name>RHaileron</object-name>
        <object-name>LHaileron</object-name>
        <object-name>RHflap</object-name>
        <object-name>LHflap</object-name>
        <object-name>LHfront.door</object-name>
        <object-name>RHfront.door</object-name>
        <object-name>RHrear.door</object-name>
        <object-name>LHrear.door</object-name>
    </effect>

    <effect>
        <inherits-from>Aircraft/dhc2/Models/Effects/glass-uber</inherits-from>
        <object-name>LHFdoor.glass</object-name>
        <object-name>RHFdoor.glass</object-name>
        <object-name>RRglass</object-name>
        <object-name>LRglass</object-name>
        <object-name>Glass</object-name>
      </effect>

    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>Fastprop</object-name>
        <object-name>Slowprop</object-name>
        <object-name>LndLight</object-name>
        <object-name>LHreg</object-name>
         <object-name>RHreg</object-name>
    </effect>

    <animation>
        <type>select</type>
        <object-name>LHFdoor.glass</object-name>
        <object-name>RHFdoor.glass</object-name>
        <object-name>RRglass</object-name>
        <object-name>LRglass</object-name>
        <object-name>Glass</object-name>
        <condition>
            <not>
                <property>sim/current-view/internal</property>
            </not>
        </condition>
    </animation>

     <animation>
        <type>select</type>
        <object-name>LndLight</object-name>
        <condition>
            <property>systems/electrical/outputs/landing-lights</property>
        </condition>
    </animation>

    <animation>
        <type>flash</type>
        <object-name>LndLight</object-name>
        <center>
            <x-m>-0.811</x-m>
            <y-m>-3.689</y-m>
            <z-m>1.094</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <power>2</power>
        <factor>1</factor>
        <offset>0</offset>
        <two-sides type="bool">false</two-sides>
        <min>0.2</min>
        <max>1</max>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Fastprop</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/rpm</property>
                    <value>1500</value>
                </greater-than>
                <greater-than>
                    <property>sim/current-view/view-number</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Slowprop</object-name>
        <condition>
            <less-than>
                <property>engines/engine/rpm</property>
                <value>1501</value>
            </less-than>
            <greater-than>
                <property>engines/engine/rpm</property>
                <value>350</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Blade1</object-name>
        <object-name>Blade2</object-name>
        <condition>
            <less-than>
                <property>engines/engine[0]/rpm</property>
                <value>351</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>Blade1</object-name>
        <object-name>Blade2</object-name>
        <property>engines/engine[0]/rpm</property>
        <center>
            <x-m>2.623</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.083</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>


    <animation>
        <type>spin</type>
        <object-name>Slowprop</object-name>
        <property>engines/engine/rpm</property>
        <factor>0.5</factor>
        <center>
            <x-m>2.623</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.083</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
