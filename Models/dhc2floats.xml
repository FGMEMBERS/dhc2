 <animation>
  <type>select</type>
  <object-name>cabin</object-name>
  <condition>
   <equals>
    <property>/sim/current-view/view-number</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>Cargo</object-name>
  <condition>
   <greater-than>
    <property>/yasim/cargo</property>
    <value>0.000</value>
   </greater-than>
  </condition>
 </animation>

  <animation>
  <type>select</type>
  <object-name>Fastprop</object-name>
  <condition>
   <greater-than>
    <property>/engines/engine[0]/rpm</property>
    <value>1200</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Slowprop</object-name>
  <condition>
   <less-than>
    <property>/engines/engine/rpm</property>
    <value>1201</value>
   </less-than>
   <greater-than>
    <property>/engines/engine/rpm</property>
    <value>500</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
