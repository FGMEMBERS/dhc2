        </volume>
      </click>
 
            <click> 
        <name>Nav Light On</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <property>controls/switches/nav-lights</property>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

                  <click> 
        <name>Nav Light Off</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <not>
	 <property>controls/switches/nav-lights</property>
	  </not>
	  </condition>
        <volume> 
