	 <not>
	 <property>controls/switches/pitot-heat</property>
	  </not>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
 
            <click> 
        <name>Landing Light On</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <property>controls/switches/landing-light</property>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

                  <click> 
        <name>Landing Light Off</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <not>
	 <property>controls/switches/landing-light</property>
	  </not>
	  </condition>
        <volume> 
