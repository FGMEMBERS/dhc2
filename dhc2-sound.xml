      <click> 
        <name>FuelSelector</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition> 
	<not>
          <property>consumables/fuel/tank[0]/selected</property>
        </not>
	</condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>FuelSelector 1</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition> 
          <property>consumables/fuel/tank[0]/selected</property>
          </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>FuelSelector 2</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition> 
          <property>consumables/fuel/tank[1]/selected</property>
          </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>FuelSelector 3</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition> 
          <property>consumables/fuel/tank[2]/selected</property>
          </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      
      <click> 
        <name>Magnetos-off</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <not>
          <property>controls/engines/engine/magnetos</property>
         </not>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
	 
      <click> 
        <name>Magnetos-R</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	<equals>
	 <property>controls/engines/engine/magnetos</property>
         <value>1</value>
	 </equals>
	 </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
  
      <click> 
        <name>Magnetos-L</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <equals>
	 <property>controls/engines/engine/magnetos</property>
         <value>2</value>
	 </equals>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
  
      <click> 
        <name>Magnetos-B</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <equals>
	 <property>controls/engines/engine/magnetos</property>
         <value>3</value>
	 </equals>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

            <click> 
        <name>Pitot Heat On</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
        <condition>
	 <property>controls/switches/pitot-heat</property>
	  </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
 
            <click> 
        <name>Pitot Heat Off</name>
        <path>Aircraft/dhc2/Sound/click.wav</path>
